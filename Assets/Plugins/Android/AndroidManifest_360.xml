  <application
      android:theme="@android:style/Theme.NoTitleBar.Fullscreen" android:icon="@drawable/app_icon" android:label="@string/app_name" android:debuggable="false" >
    
    <!-- 提供横屏或者竖屏游戏入口 -->
    <!-- 请在主界面对应的activity上，添加属性android:configChanges="orientation|keyboardHidden|screenSize"
              ，否则横竖屏发生切换时，activity会重新创建，会引起一些问题。 -->
    <activity
        android:name="com.qianhuan.yxgsd.qihoo360.SdkMainActivity" android:label="@string/app_name" android:screenOrientation="fullSensor" android:launchMode="singleTask" android:configChanges="mcc|mnc|locale|touchscreen|keyboard|keyboardHidden|navigation|orientation|screenLayout|uiMode|screenSize|smallestScreenSize|fontScale">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
        <category android:name="android.intent.category.LEANBACK_LAUNCHER" />
      </intent-filter>
      <meta-data android:name="unityplayer.UnityActivity" android:value="true" />
      <meta-data android:name="unityplayer.ForwardNativeEventsToDalvik" android:value="false" />
    </activity>

    <!-- PortraitSdkUserActivity，竖屏：用于演示如何使用360SDK和Demo应用服务器登录、定额支付、不定额支付；并提供流程测试入口。 -->
    <activity
        android:name=".activity.PortraitSdkUserActivity"
        android:configChanges="fontScale|orientation|keyboardHidden|locale|navigation|screenSize|uiMode"
        android:screenOrientation="portrait" >
    </activity>

    <!-- LandscapeSdkUserActivity，横屏：用于演示如何使用360SDK和Demo应用服务器登录、定额支付、不定额支付；并提供流程测试入口。 -->
    <activity
        android:name=".activity.LandscapeSdkUserActivity"
        android:configChanges="fontScale|orientation|keyboardHidden|locale|navigation|screenSize|uiMode"
        android:screenOrientation="landscape" >
    </activity>
    <!-- FlowTestLoginActivity，用于流程测试第一步，设置登录参数，测试某应用使用360SDK和DemoAppServer的登录过程。登录成功后进入流程测试第二步。 -->
    <activity
        android:name=".activity.FlowTestLoginActivity"
        android:configChanges="fontScale|orientation|keyboardHidden|locale|navigation|screenSize|uiMode" >
    </activity>

    <!-- FlowTestPayActivity，用于流程测试第二步，设置定额支付参数，测试某应用使用360SDK的定额支付过程。 -->
    <activity
        android:name=".activity.FlowTestPayActivity"
        android:configChanges="fontScale|orientation|keyboardHidden|locale|navigation|screenSize|uiMode" >
    </activity>

    <!-- 添加360SDK必需的activity：com.qihoopay.insdk.activity.ContainerActivity -->
